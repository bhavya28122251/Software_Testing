{
  "name": "student-management-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "seed": "node seed.js",
    "test:unit": "mocha --recursive test/unit",
    "test:integration": "mocha --require test/setup.js --recursive test/integration --timeout 10000 --exit",
    "test": "npm run test:unit",
    "coverage": "nyc --reporter=lcov --reporter=text npm test",
    "stryker": "stryker run",
    "stryker:integration": "stryker run stryker.integration.conf.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/mocha-runner": "^9.4.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "supertest": "^7.1.4"
  }
}
